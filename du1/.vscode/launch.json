	"version": "0.2.0",
	"configurations": [
	{
		"name": "(gdb) Steg-decode",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/steg-decode",
		"args": ["du1-obrazek.ppm"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
			{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			}
		]
	},
		{
			"name": "(gdb) Primes",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/primes",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}